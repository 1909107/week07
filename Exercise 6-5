#include <stdio.h>
int main() {
int año;
printf("Ingrese un año ");
scanf("%i", &año);
if (año % 400 == 0) {
printf("%i no es un salto de año ", año);
}
else if (año % 100 == 0) {
printf("%i es un salto de año", año);
}
else if (año % 4 == 0) {
printf("%i es un salto de año", año);
}
else {
printf("%i no es un salto de año", año);
}
return 0;
}
